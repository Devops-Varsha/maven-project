pipeline
{
agent any
stages
{
 stage ('scm checkout')
 { steps {
     git branch: 'master', url: 'https://github.com/Devops-Varsha/maven-project.git'
         }
 }
 
stage ('create package')
{
    steps {
     withMaven(jdk: 'JAVA_HOME', maven: 'MAVEN_HOME') {
      sh 'mvn package'
}
    }
}



}}

