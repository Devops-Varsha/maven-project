pipeline
{
    agent any
    stages{
        stage('SCM Checkout')
        {
            steps{ git branch: 'master', url: 'https://github.com/Devops-Varsha/maven-project'}
        }
        stage('Build the code')
        {
            steps{ 
                withMaven(jdk: 'JAVA_HOME', maven: 'MAVEN_HOME') 
                {
                     sh 'mvn package'
                }
            }
        }
    }
}